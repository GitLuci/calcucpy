print("introduza o primeiro numero:")
num1 = int(input())
print("introduza o segundo numero:")
num2 = int(input())
soma = num1 + num2
mult = num1 * num2
elev = num1 ** num2
div = int(num1 / num2)
porc1 = num1 ** 0.5
porc2 = num2 ** 0.5
print(f"A mult dos dois numeros é de {mult}")
print(f"A elev dos dois numeros é de {elev}")
print(f"A div dos dois numeros é de {div}")
print(f"A raiz1 dos dois numeros é de {porc1}")
print(f"A raiz2 dos dois numeros é de {porc2}")

input()


t = int(input("digite o valor de tempo: "))
v = int(input("digite o valor da velocidade: "))
c = t * v
cons = ((8 * c) / 100)
f = (c / t)
print("O total de litros é de: ", cons, "litros, e a distancia é de: ", c, "km, e a velocidade media é de: ", f, );

print(f'o consumo medio em litros é de: {cons} e a distancia é de {c}km, e a media da velocidade é de {f}km/h')




//exercio triangulos

print("Digite os valores dos lados do triangulo respectivamente: ")
a1 = int(input())
a2 = int(input())
a3 = int(input())
if  a1 == a2 == a3:
    print("Este é um triangulo escaleno")
elif
    if a1 == a2 or a2 == a3 or a3 == a1:
        print("Este é um triangulo aaa")
    elif
        if a1 != a2 != a3:
            print("Este é um triangulo bbb")
        elif 
        
        print("ERROR")
